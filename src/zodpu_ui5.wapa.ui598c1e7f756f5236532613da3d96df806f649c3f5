\n\tonSendRequest() {\n\t\tthis.clearResult();\n\t\tif (this.localData.resourceType === \"entity\") {\n\t\t\tswitch (this.localData.selectedMethod) {\n\t\t\t\tcase \"GET\":\n\t\t\t\t\tvoid this.execEntityRead();\n\t\t\t\t\tbreak;\n\t\t\t\tcase \"GETBY\":+
