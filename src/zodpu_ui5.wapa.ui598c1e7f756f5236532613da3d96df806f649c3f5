 */\n\tprivate handleFunctionChanged() {\n\t\tthis.resetFunctionInputs();\n\n\t\tconst selectedFunction = this.localData.selectedServiceFunctions.find(\n\t\t\t(func) => func.name === this.localData.selectedFunctionName\n\t\t);\n\n\t\t(this.getView().getM+
