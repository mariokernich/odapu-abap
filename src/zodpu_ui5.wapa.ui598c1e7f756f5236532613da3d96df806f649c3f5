\t\t\t\tcase \"GET\":\n\t\t\t\t\tvoid this.execEntityRead();\n\t\t\t\t\tbreak;\n\t\t\t\tcase \"GETBY\":\n\t\t\t\t\tvoid this.execEntityGetSingle();\n\t\t\t\t\tbreak;\n\t\t\t\tcase \"POST\":\n\t\t\t\t\tvoid this.execEntityCreate();\n\t\t\t\t\tbreak;\n\t\t+
