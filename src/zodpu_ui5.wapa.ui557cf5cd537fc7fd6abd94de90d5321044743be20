leClass(\"sapUiResponsivePadding\");\n\n\t\tdialog.addContent(toolbar);\n\t\tdialog.addContent(table);\n\t\tdialog.setModel(serviceModel, \"services\");\n\n\t\tdialog.open();\n\t\tdialog.setBusy(true);\n\t\tservices = await this.requests.getServices();\n+
