t\tconst selectedEntity = this.selectedService.entities?.find(\n\t\t\t(entity) => entity.name === this.localData.selectedEntityName\n\t\t);\n\n\t\tif (selectedEntity) {\n\t\t\tthis.localData.selectedEntityProperties.properties =\n\t\t\t\tselectedEntity.p+
