.showAddHeaderDialog();\n\t\tconst headers = (\n\t\t\tthis.getView()?.getModel(\"requestHeaders\") as JSONModel\n\t\t).getProperty(\"/\") as RequestHeader[];\n\t\tconst existingHeader = headers.find((h) => h.key === header.key);\n\t\tif (existingHeader) +
