<core:FragmentDefinition                                                                                                                                                                                                                                       
    xmlns="sap.m"                                                                                                                                                                                                                                              
    xmlns:core="sap.ui.core"                                                                                                                                                                                                                                   
>                                                                                                                                                                                                                                                              
    <Dialog                                                                                                                                                                                                                                                    
        title="{i18n>dlg.title.selectProjectType}"                                                                                                                                                                                                             
        draggable="true"                                                                                                                                                                                                                                       
        initialFocus="idODataGenericTile"                                                                                                                                                                                                                      
        class="sapUiContentPadding"                                                                                                                                                                                                                            
        busyIndicatorDelay="0"                                                                                                                                                                                                                                 
        contentWidth="400px"                                                                                                                                                                                                                                   
    >                                                                                                                                                                                                                                                          
        <content>                                                                                                                                                                                                                                              
            <VBox                                                                                                                                                                                                                                              
                width="100%"                                                                                                                                                                                                                                   
                height="100%"                                                                                                                                                                                                                                  
                fitContainer="true"                                                                                                                                                                                                                            
            >                                                                                                                                                                                                                                                  
                <Text                                                                                                                                                                                                                                          
                    text="{i18n>msg.selectProjectType}"                                                                                                                                                                                                        
                />                                                                                                                                                                                                                                             
                <HBox                                                                                                                                                                                                                                          
                    justifyContent="SpaceBetween"                                                                                                                                                                                                              
                    class="sapUiSmallMarginTop"                                                                                                                                                                                                                
                >                                                                                                                                                                                                                                              
                    <GenericTile                                                                                                                                                                                                                               
                        header="{i18n>lbl.odata}"                                                                                                                                                                                                              
                        subheader="{i18n>lbl.odataSubheader}"                                                                                                                                                                                                  
                        frameType="OneByOne"                                                                                                                                                                                                                   
                        press=".onSelectOData"                                                                                                                                                                                                                 
                        class="sapUiTinyMarginEnd"                                                                                                                                                                                                             
                        id="idODataGenericTile"                                                                                                                                                                                                                
                    >                                                                                                                                                                                                                                          
                        <tileContent>                                                                                                                                                                                                                          
                            <TileContent>                                                                                                                                                                                                                      
                                <content>                                                                                                                                                                                                                      
                                    <VBox>                                                                                                                                                                                                                     
                                        <core:Icon                                                                                                                                                                                                             
                                            src="sap-icon://add-activity"                                                                                                                                                                                      
                                            size="2rem"                                                                                                                                                                                                        
                                        />                                                                                                                                                                                                                     
                                    </VBox>                                                                                                                                                                                                                    
                                </content>                                                                                                                                                                                                                     
                            </TileContent>                                                                                                                                                                                                                     
                        </tileContent>                                                                                                                                                                                                                         
                    </GenericTile>                                                                                                                                                                                                                             
                    <GenericTile                                                                                                                                                                                                                               
                        header="{i18n>lbl.apc}"                                                                                                                                                                                                                
                        subheader="{i18n>lbl.apcSubheader}"                                                                                                                                                                                                    
                        frameType="OneByOne"                                                                                                                                                                                                                   
                        press=".onSelectAPC"                                                                                                                                                                                                                   
                        class="sapUiTinyMarginBegin"                                                                                                                                                                                                           
                    >                                                                                                                                                                                                                                          
                        <tileContent>                                                                                                                                                                                                                          
                            <TileContent>                                                                                                                                                                                                                      
                                <content>                                                                                                                                                                                                                      
                                    <VBox>                                                                                                                                                                                                                     
                                        <core:Icon                                                                                                                                                                                                             
                                            src="sap-icon://add-activity"                                                                                                                                                                                      
                                            size="2rem"                                                                                                                                                                                                        
                                        />                                                                                                                                                                                                                     
                                    </VBox>                                                                                                                                                                                                                    
                                </content>                                                                                                                                                                                                                     
                            </TileContent>                                                                                                                                                                                                                     
                        </tileContent>                                                                                                                                                                                                                         
                    </GenericTile>                                                                                                                                                                                                                             
                </HBox>                                                                                                                                                                                                                                        
            </VBox>                                                                                                                                                                                                                                            
        </content>                                                                                                                                                                                                                                             
        <buttons>                                                                                                                                                                                                                                              
            <Button                                                                                                                                                                                                                                            
                text="{i18n>btn.about}"                                                                                                                                                                                                                        
                icon="sap-icon://information"                                                                                                                                                                                                                  
                press=".onShowAbout"                                                                                                                                                                                                                           
            />                                                                                                                                                                                                                                                 
            <Button                                                                                                                                                                                                                                            
                text="{i18n>btn.cancel}"                                                                                                                                                                                                                       
                icon="sap-icon://decline"                                                                                                                                                                                                                      
                press=".onCancel"                                                                                                                                                                                                                              
            />                                                                                                                                                                                                                                                 
        </buttons>                                                                                                                                                                                                                                             
    </Dialog>                                                                                                                                                                                                                                                  
</core:FragmentDefinition>&nbsp;                                                                                                                                                                                                                               