reject(reason?: string) {\n        this.promiseHelper?.reject(reason || \"Cancelled\");\n    }\n\n    destroy() {\n        super.destroy();\n        this.dialog?.destroy();\n        this.promiseHelper?.reject(\"destroy routine\");\n    }\n}\n\nexport def+
